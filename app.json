{
  "expo": {
    "name": "DetoxGestureApp",
    "slug": "DetoxGestureApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "detoxgestureapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.DetoxGestureApp"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.DetoxGestureApp",
      "namespace": "com.anonymous.DetoxGestureApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
